<h1>Hello <%= nama %>! we're learning NodeJS!</h1>
<p>Happy Coding, Happy Learning</p>


<h4>Daftar Mahasiswa</h4>
<% if (mahasiswa.length === 0) { %>
  <p>Data mahasiswa masih kosong</p>
<% } %>

<% mahasiswa.forEach(mhs => { %>
  <ul>
    <li>Nama: <%= mhs.nama %></li>
    <li>Email: <%= mhs.email %></li>
  </ul>
<% }) %>